fullnameOverride: cert-manager-issuers

issuersMap:
  letsencrypt:
    kind: ClusterIssuer
    namespace: cert-manager
    spec:
      acme:
        server: https://acme-v02.api.letsencrypt.org/directory
        email: dangeron@gmail.com
        privateKeySecretRef:
          name: letsencrypt
        solvers:
        - http01:
            ingress:
              class: nginx
